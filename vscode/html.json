{
	// HTML Elements
	"Div.tab": {
		"prefix": "tab",
		"body": [
			"<div class=\"tab\">",
			"    $0",
			"</div>"
		],
		"description": "Div.tab"
	},
	"Div.center > img": {
		"prefix": "centerimg",
		"body": [
			"<div class=\"center\"><img src=\"img/$1\" width=\"${2:100%}\"></div>"
		],
		"description": "Div.center > img"
	},
	"Div.box > ol": {
		"prefix": "boxol",
		"body": [
			"<div class=\"box\">",
			"    <ol class=\"upper\">",
			"        <li>$0</li>",
			"    </ol>",
			"</div>"
		],
		"description": "Div.box > ol"
	},
	"Div.box-list & text": {
		"prefix": "boxlist",
		"body": [
			"Which option only contains every correct statement in the &lt;List&gt;?",
            "<small>[${1:4} points]</small>",
            "<div class=\"box-list\">",
            "    <ol class=\"alpha\">",
            "        <li>$2</li>",
            "        <li>$3</li>",
            "        <li>$4</li>",
            "    </ol>",
            "</div>",
            "<ol class=\"col-3\">",
            "    <li>a</li>",
            "    <li>a, b</li>",
            "    <li>a, c</li>",
            "    <li>b, c</li>",
            "    <li>a, b, c</li>",
            "</ol>"
		],
		"description": "Div.box-list & text"
	},

	// Miscellaneous symbols
	"Degree symbol": {
		"prefix": "deg",
		"body": [
			"Â°"
		],
		"description": "Degree symbol"
	},

	// MathJax stuff
	"TeX": {
		"prefix": "x",
		"body": [
			"\\($1\\)$0"
		],
		"description": "TeX"
	},
	
	// Displaystyle stuff
	"Frac": {
		"prefix": "fr",
		"body": [
			"\\frac{$1}{$2}$0"
		],
		"description": "Frac"
	},
	"Displaystyle Frac": {
		"prefix": "dfr",
		"body": [
			"\\dfrac{$1}{$2}$0"
		],
		"description": "Displaystyle Frac"
	},
	"Limit": {
		"prefix": "lim",
		"body": [
			"\\lim_{${1:x}\\to${2:\\infty}}$0"
		],
		"description": "Limit"
	},
	"Displaystyle Limit": {
		"prefix": "dlim",
		"body": [
			"\\displaystyle\\lim_{${1:x}\\to${2:\\infty}}$0"
		],
		"description": "Displaystyle Limit"
	},
	"Integral": {
		"prefix": "int",
		"body": [
			"\\int_{${1:0}}^{${2:1}}$0"
		],
		"description": "Integral"
	},
	"Displaystyle Integral": {
		"prefix": "dint",
		"body": [
			"\\displaystyle\\int_{${1:0}}^{${2:1}}$0"
		],
		"description": "Displaystyle Integral"
	},
	"Sum": {
		"prefix": "sum",
		"body": [
			"\\sum_{${1:n}=${2:1}}^{${3:\\infty}}$0"
		],
		"description": "Sum"
	},
	"Displaystyle Sum": {
		"prefix": "dsum",
		"body": [
			"\\displaystyle\\sum_{${1:n}=${2:1}}^{${3:\\infty}}$0"
		],
		"description": "Displaystyle Sum"
	},

	// Mathrm stuff
	"Mathrm": {
		"prefix": "rm",
		"body": [
			"\\mathrm{${1:A}}$0"
		],
		"description": "Mathrm"
	},
	"TeX Mathrm": {
		"prefix": "xrm",
		"body": [
			"\\(\\mathrm{${1:A}}\\)$0"
		],
		"description": "TeX Mathrm"
	},
	"Overline": {
		"prefix": "ovl",
		"body": [
			"\\overline{\\mathrm{${1:AB}}}$0"
		],
		"description": "Overline"
	},
	"Overrightarrow": {
		"prefix": "ovr",
		"body": [
			"\\overrightarrow{\\mathrm{${1:AB}}}$0"
		],
		"description": "Overrightarrow"
	},

	// Begin-End stuff
	"Cases": {
		"prefix": "cases",
		"body": [
			"\\begin{cases}",
			"    ${1:x} &\\; (${2:x &lt; 0}) \\\\\\",
			"    ${3:x} &\\; (${4:x \\geq 0})",
			"\\end{cases}$0"
		],
		"description": "Cases"
	},
	"Pmatrix": {
		"prefix": "pmat",
		"body": [
			"\\begin{pmatrix} ${1:0&0} \\\\\\ ${2:0&0} \\end{pmatrix}$0"
		],
		"description": "Pmatrix"
	},

	// Parentheses stuff
	"Parentheses ()": {
		"prefix": "par",
		"body": [
			"\\left(\\!$1\\!\\right)$0"
		],
		"description": "Parentheses ()"
	},

	"Fbox": {
		"prefix": "fbox",
		"body": [
			"\\fbox{\\(\\;(${1:\\alpha})\\;\\)}$0"
		],
		"description": "Fbox"
	}
}
